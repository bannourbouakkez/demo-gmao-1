{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gmao": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/gmao",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/@pages/styles",
                "src/pages_ui/scss",
                
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              },
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              

              "src/theme.less",
 

              "src/styles.scss",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/plugins/bootstrap/css/bootstrap.min.css",
              "src/assets/plugins/codrops-stepsform/css/component.css"



            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/popper.js/dist/umd/popper.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/focus-visible/dist/focus-visible.min.js",
              "node_modules/scrollreveal/dist/scrollreveal.js",
              "node_modules/echarts/dist/echarts.min.js",
              "src/app/@pages/assets/javascript/pages.js",
              "node_modules/masonry-layout/dist/masonry.pkgd.min.js",
              "node_modules/isotope-layout/dist/isotope.pkgd.min.js",
              "node_modules/imagesloaded/imagesloaded.pkgd.js",
              "src/assets/plugins/modernizr.custom.js",
              "src/assets/plugins/codrops-stepsform/js/classie.js",
              "src/assets/plugins/codrops-stepsform/js/stepsForm.js",
              "src/assets/plugins/skycons/skycons.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gmao:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gmao:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gmao:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/scrollreveal/dist/scrollreveal.js",
              "node_modules/echarts/dist/echarts.min.js",
              "src/app/@pages/assets/javascript/pages.js",
              "node_modules/masonry-layout/dist/masonry.pkgd.min.js",
              "node_modules/isotope-layout/dist/isotope.pkgd.min.js",
              "node_modules/imagesloaded/imagesloaded.pkgd.js",
              "src/assets/plugins/modernizr.custom.js",
              "src/assets/plugins/codrops-stepsform/js/classie.js",
              "src/assets/plugins/codrops-stepsform/js/stepsForm.js",
              "src/assets/plugins/skycons/skycons.js"
            ],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "src/assets/plugins/bootstrap/css/bootstrap.min.css",
              "src/assets/plugins/font-awesome/css/font-awesome.min.css",
              "src/assets/plugins/codrops-stepsform/css/component.css",

              "src/theme.less"

              

            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              },
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "gmao:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gmao:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "gmao",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}